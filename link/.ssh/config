    Compression yes
    CompressionLevel 9
    ServerAliveInterval 300
    ProxyCommand proxytunnel -q -X -p http-proxy.health.ge.com:88 -r whidden.net:443 -R jp -d whidden.net:22
    ServerAliveInterval 30

